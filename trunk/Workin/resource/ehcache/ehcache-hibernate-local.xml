<?xml version="1.0" encoding="UTF-8"?>
<ehcache>

	<diskStore path="java.io.tmpdir/ehcache/workin" />

	<defaultCache maxElementsInMemory="10000" overflowToDisk="true" eternal="false" memoryStoreEvictionPolicy="LRU"
		maxElementsOnDisk="10000000" diskExpiryThreadIntervalSeconds="600" timeToIdleSeconds="3600" timeToLiveSeconds="100000"
		diskPersistent="false" />


	<!-- Special objects setting. -->
	<cache name="org.workin.test.entity.Person" maxElementsInMemory="500" overflowToDisk="true" eternal="true" />
	<cache name="org.workin.test.entity.Address" maxElementsInMemory="500" overflowToDisk="true" eternal="true" />
</ehcache>
